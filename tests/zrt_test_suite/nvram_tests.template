###########################################################

[env]
{ENVIRONMENT} 
name=usable, value=1\x2C2\x2C3/"1\x2C2\x2C3"

[args]
{COMMAND_LINE}

[mapping]
{MAPPING}

[fstab]
{FSTAB}
channel=/dev/mount/gcov.gcda.tar, mountpoint=/, access=ro, removable=no
channel=/dev/mount/gcov.gcda.tar, mountpoint=/home/zvm/git/zrt, access=wo, removable=no

[time]
{SECONDS}

[debug]
verbosity=2

[precache]
{PRECACHE}